<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
              http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         version="2.5"
         xmlns="http://java.sun.com/xml/ns/javaee">

  <!-- Servlets -->
  <servlet>
    <servlet-name>greetServlet</servlet-name>
    <servlet-class>com.zxgj.logadmin.server.GreetingServiceImpl</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>greetServlet</servlet-name>
    <url-pattern>/webapp/greet</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>logLevel</servlet-name>
    <servlet-class>com.zxgj.logadmin.server.LogLevelServiceImpl</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>logLevel</servlet-name>
    <url-pattern>/webapp/loglevel</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>secLog</servlet-name>
    <servlet-class>com.zxgj.logadmin.server.SECLogServiceImpl</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>secLog</servlet-name>
    <url-pattern>/webapp/seclog</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>eapLog</servlet-name>
    <servlet-class>com.zxgj.logadmin.server.EAPLogServiceImpl</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>eapLog</servlet-name>
    <url-pattern>/webapp/eaplog</url-pattern>
  </servlet-mapping>
  
   <servlet>
    <servlet-name>searchService</servlet-name>
    <servlet-class>com.zxgj.logadmin.server.SearchServiceImpl</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>searchService</servlet-name>
    <url-pattern>/webapp/searchservice</url-pattern>
  </servlet-mapping>
  <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>webapp2.html</welcome-file>
  </welcome-file-list>

</web-app>
